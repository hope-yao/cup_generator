np.mean(np.abs(sess.run(tf.gradients(loss['height'], WB['W1']),feed_dict_fix))),np.mean(np.abs(sess.run(tf.gradients(loss['bottom'], WB['W1']),feed_dict_fix))),np.mean(np.abs(sess.run(tf.gradients(loss['area'], WB['W1']),feed_dict_fix)))


sess.run([x_output[-1], open_flg[-1], loss, center_gravity, area_list], feed_dict_fix)










